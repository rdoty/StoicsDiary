<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomLayout">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="StoicColorStyle" >
        <attr name="displayName" format="string" />
        <attr name="colorPrimary" format="color" />
        <attr name="colorAccent" format="color" />
        <attr name="colorGoodFg" format="color" />
        <attr name="colorGoodBg" format="color" />
        <attr name="colorBadFg" format="color" />
        <attr name="colorBadBg" format="color" />
        <attr name="colorStatTitle" format="color" />
        <attr name="colorStatValue" format="color" />
    </declare-styleable>

    <declare-styleable name="StoicTextStyle" >
        <attr name="displayName" />
        <attr name="choicePrompt" format="string" />
        <attr name="choiceGood" format="string" />
        <attr name="choiceBad" format="string" />
        <attr name="choiceDisabledSelected" format="string" />
        <attr name="choiceDisabledUnselected" format="string" />
    </declare-styleable>
 </resources>
